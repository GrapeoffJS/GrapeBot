{
    "name": "grapebot",
    "version": "1.0.0",
    "description": "Bot for GrapeCode++ Discord Server",
    "main": "index.js",
    "scripts": {
        "start:dev": "nodemon dist/app.js",
        "build:dev": "tsc --watch",
        "build": "tsc --project tsconfig.json",
        "deploy:forbidden_nicknames": "ts-node -r tsconfig-paths/register src/scripts/deploy-forbidden-commands.ts"
    },
    "engines": {
        "node": ">=17.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GrapeoffJS/GrapeBot.git"
    },
    "author": "GrapeoffJS",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/GrapeoffJS/GrapeBot/issues"
    },
    "homepage": "https://github.com/GrapeoffJS/GrapeBot#readme",
    "dependencies": {
        "@discordjs/builders": "^0.15.0",
        "@discordjs/rest": "^0.5.0",
        "@tensorflow/tfjs-node": "^3.18.0",
        "@typegoose/typegoose": "^9.9.0",
        "axios": "^0.27.2",
        "class-validator": "^0.13.2",
        "discord-api-types": "^0.33.4",
        "discord.js": "^13.8.0",
        "dotenv": "^16.0.1",
        "mongoose": "^6.3.5",
        "nsfwjs": "^2.4.1"
    },
    "devDependencies": {
        "@types/": "tensorflow/tfjs-node",
        "@types/axios": "^0.14.0",
        "@types/dotenv": "^8.2.0",
        "@types/node": "^17.0.40",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "eslint": "^8.17.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-security": "^1.5.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sonarjs": "^0.13.0",
        "eslint-plugin-unicorn": "^42.0.0",
        "module-alias": "^2.2.2",
        "nodemon": "^2.0.16",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.8.1",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.3"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/app.ts",
        "ext": "ts, js"
    }
}
